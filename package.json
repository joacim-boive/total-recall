{
  "name": "total-recall",
  "displayName": "Total Recall",
  "description": "It allows you to recall your context across branches.",
  "version": "0.5.4",
  "publisher": "jboive",
  "icon": "images/icon-hd.png",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
    ],
    "configuration": {
      "properties": {
        "extension-testing.tabGroups": {
          "type": "boolean",
          "default": true,
          "description": "Enable tab groups functionality"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "vite build",
    "watch": "vite build --watch",
    "package": "vsce package",
    "lint": "eslint src --ext ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitest/ui": "^2.1.2",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^9.12.0",
    "globals": "^15.10.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joacim-boive/total-recall.git"
  },
  "license": "MIT"
}
